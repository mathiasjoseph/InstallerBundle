<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <imports>
        <import resource="services/provider.xml"/>
    </imports>
    <services>
        <service id="adevis_bundle_installer.command.installer_command" class="Miky\Bundle\InstallerBundle\Command\InstallerCommand">
            <argument id="adevis_installer.provider.installer_provider" type="service"/>
            <tag name="console.command"/>
        </service>
    </services>
</container>
